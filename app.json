{
  "expo": {
    "name": "motiwithexpo",
    "slug": "moti-with-expo",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#F5F5DC"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "description": "UI example in Expo with animations using Moti ",
    "githubUrl": "https://github.com/ricardoleme/motiWithExpo"
  }
}
